<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".content.new_order.fragments.TankDispatchFragment">


    <androidx.cardview.widget.CardView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="@dimen/_8sdp"
        app:cardCornerRadius="@dimen/_4sdp"
        app:cardElevation="@dimen/_8sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="@dimen/_8sdp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Despacho de Cisterna"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_15ssp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:orientation="horizontal">

                    <EditText
                        android:id="@+id/etNumTank1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:hint="N° Cisterna"
                        android:inputType="number"
                        android:lines="1"
                        android:maxLines="1"
                        android:textSize="@dimen/_11ssp" />

                    <EditText
                        android:id="@+id/etTimeEnd1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:hint="Hora Salida"
                        android:focusable="false"
                        android:focusableInTouchMode="false"
                        android:clickable="true"
                        android:lines="1"
                        android:maxLines="1"
                        android:textSize="@dimen/_11ssp" />

                    <EditText
                        android:id="@+id/etPozo1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:hint="Pozo"
                        android:lines="1"
                        android:maxLines="1"
                        android:textSize="@dimen/_11ssp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:orientation="horizontal">

                    <EditText
                        android:id="@+id/etCantDesp1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:hint="Cant Despachada"
                        android:inputType="number"
                        android:lines="1"
                        android:maxLines="1"
                        android:textSize="@dimen/_11ssp" />

                    <EditText
                        android:id="@+id/etLugDesc1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:hint="Lugar Descarga"
                        android:lines="1"
                        android:maxLines="1"
                        android:textSize="@dimen/_11ssp" />

                    <EditText
                        android:id="@+id/etCist1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:hint="Cisterna"
                        android:lines="1"
                        android:maxLines="1"
                        android:textSize="@dimen/_11ssp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:orientation="horizontal">

                    <EditText
                        android:id="@+id/etProced1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:hint="Procedencia"
                        android:lines="1"
                        android:maxLines="1"
                        android:textSize="@dimen/_11ssp" />

                    <EditText
                        android:id="@+id/etTimeInit1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:hint="Hora Llegada"
                        android:focusable="false"
                        android:focusableInTouchMode="false"
                        android:clickable="true"
                        android:lines="1"
                        android:maxLines="1"
                        android:textSize="@dimen/_11ssp" />

                    <EditText
                        android:id="@+id/etPozo11"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:hint="Pozo"
                        android:lines="1"
                        android:maxLines="1"
                        android:textSize="@dimen/_11ssp" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_16sdp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Despacho de Cisterna"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_15ssp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:orientation="horizontal">

                    <EditText
                        android:id="@+id/etNumTank2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:hint="N° Cisterna"
                        android:inputType="number"
                        android:lines="1"
                        android:maxLines="1"
                        android:textSize="@dimen/_11ssp" />

                    <EditText
                        android:id="@+id/etTimeEnd2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:hint="Hora Salida"
                        android:focusable="false"
                        android:focusableInTouchMode="false"
                        android:clickable="true"
                        android:lines="1"
                        android:maxLines="1"
                        android:textSize="@dimen/_11ssp" />

                    <EditText
                        android:id="@+id/etPozo2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:hint="Pozo"
                        android:lines="1"
                        android:maxLines="1"
                        android:textSize="@dimen/_11ssp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:orientation="horizontal">

                    <EditText
                        android:id="@+id/etCantDesp2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:hint="Cant Despachada"
                        android:inputType="number"
                        android:lines="1"
                        android:maxLines="1"
                        android:textSize="@dimen/_11ssp" />

                    <EditText
                        android:id="@+id/etLugDesc2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:hint="Lugar Descarga"
                        android:lines="1"
                        android:maxLines="1"
                        android:textSize="@dimen/_11ssp" />

                    <EditText
                        android:id="@+id/etCist2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:hint="Cisterna"
                        android:lines="1"
                        android:maxLines="1"
                        android:textSize="@dimen/_11ssp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:orientation="horizontal">

                    <EditText
                        android:id="@+id/etProced2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:hint="Procedencia"
                        android:lines="1"
                        android:maxLines="1"
                        android:textSize="@dimen/_11ssp" />

                    <EditText
                        android:id="@+id/etTimeInit2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:hint="Hora Llegada"
                        android:focusable="false"
                        android:focusableInTouchMode="false"
                        android:clickable="true"
                        android:lines="1"
                        android:maxLines="1"
                        android:textSize="@dimen/_11ssp" />

                    <EditText
                        android:id="@+id/etPozo22"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:hint="Pozo"
                        android:lines="1"
                        android:maxLines="1"
                        android:textSize="@dimen/_11ssp" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>